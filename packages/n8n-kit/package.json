{
	"name": "@vahor/n8n-kit",
	"version": "0.4.0",
	"description": "Build n8n workflows using code",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Vahor/n8n-kit.git",
		"directory": "packages/n8n-kit"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./nodes": {
			"import": {
				"types": "./dist/nodes/index.d.mts",
				"default": "./dist/nodes/index.mjs"
			},
			"require": {
				"types": "./dist/nodes/index.d.cts",
				"default": "./dist/nodes/index.cjs"
			}
		},
		"./workflow": {
			"import": {
				"types": "./dist/workflow/index.d.mts",
				"default": "./dist/workflow/index.mjs"
			},
			"require": {
				"types": "./dist/workflow/index.d.cts",
				"default": "./dist/workflow/index.cjs"
			}
		},
		"./credentials/generated": {
			"import": {
				"types": "./dist/credentials/generated.d.mts",
				"default": "./dist/credentials/generated.mjs"
			},
			"require": {
				"types": "./dist/credentials/generated.d.cts",
				"default": "./dist/credentials/generated.cjs"
			}
		},
		"./utils": {
			"import": {
				"types": "./dist/utils/index.d.mts",
				"default": "./dist/utils/index.mjs"
			},
			"require": {
				"types": "./dist/utils/index.d.cts",
				"default": "./dist/utils/index.cjs"
			}
		},
		"./nodes/generated": {
			"import": {
				"types": "./dist/nodes/generated.d.mts",
				"default": "./dist/nodes/generated.mjs"
			},
			"require": {
				"types": "./dist/nodes/generated.d.cts",
				"default": "./dist/nodes/generated.cjs"
			}
		},
		"./logger": {
			"import": {
				"types": "./dist/logger.d.mts",
				"default": "./dist/logger.mjs"
			},
			"require": {
				"types": "./dist/logger.d.cts",
				"default": "./dist/logger.cjs"
			}
		}
	},
	"private": false,
	"files": [
		"dist",
		"README.md",
		"package.json"
	],
	"author": {
		"email": "me@vahor.fr"
	},
	"keywords": [
		"n8n",
		"builder",
		"workflow",
		"automation"
	],
	"dependencies": {
		"@dagrejs/dagre": "^1.1.5",
		"arktype": "catalog:",
		"lodash.clonedeep": "^4.5.0",
		"nano-spawn": "catalog:",
		"tsdown": "catalog:",
		"chalk": "catalog:",
		"slugify": "^1.6.6"
	},
	"devDependencies": {
		"@n8n/n8n-nodes-langchain": "1.108.0",
		"@types/lodash.clonedeep": "^4.5.9",
		"cheerio": "1.0.0-rc.6",
		"codemaker": "^1.113.0",
		"eventsource": "2.0.2",
		"n8n-core": "1.108.0",
		"tinyglobby": "^0.2.14",
		"typescript": "catalog:"
	},
	"scripts": {
		"prebuild": "bun run generate:entrypoint",
		"build": "bunx tsdown",
		"dev": "bunx tsdown --watch",
		"generate": "bun run generate:nodes && bun run generate:credentials && bun run generate:nodes-impl && bun run generate:entrypoint",
		"generate:entrypoint": "bun run scripts/generate-entrypoint.ts",
		"generate:nodes": "bun run scripts/generate-nodes.ts",
		"generate:nodes-impl": "bun run scripts/generate-nodes-impl.ts",
		"generate:credentials": "bun run scripts/generate-credentials.ts",
		"typecheck": "tsc --project tsconfig.check.json"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"bun": ">=1.2.20",
		"node": ">=18"
	}
}
